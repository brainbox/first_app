<!DOCTYPE html>
<html>
<head>
  <title>Survival Project</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js", "http://ajax.googleapis.com/ajax/libs/jquery/1.5/jquery.min.js" , "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js" , "rails", "highcharts", "application", "exporting" %>
 <link href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css" rel="stylesheet" type="text/css"/>
 <%= stylesheet_link_tag 'style', :media => 'screen' %>


</head>
<body>
<div class="container">
<div class="mainContent"></div>
<div class="header"></div>
<div class="nav">
<div class="nav-left"></div>
<div class="nav-main"></div>
<div class="nav-right"></div>
</div>
	<% flash.each do |key, value| %>
	  <div class="flash <%= key %>"><%= value %></div>
	<% end%>

<%= yield %>
<div class="footer">
<div class="footer-left"></div>
<div class="footer-main"></div>
<div class="footer-right"></div>
</div>
</div>
</body>
</html>
